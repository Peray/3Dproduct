/* =============================================================================
	 Menu styles
	 ========================================================================== */


#menu{
	 position: absolute;
	 width: 100%;
	 height: 100%;
	 top:0px;
	 left:0px;
	 font-family: 'divvyfont';
	 letter-spacing: 3px;
	 z-index: 1000;
	 color:#f4f4f4;
	 
}
.menu-overlay{
	width: 100%;
	height: 100%;
	background-color: rgba(1,24,39,.8);
	display: table;
	position: absolute;
}

.menu-item-container{
	display: table-cell;
	width:100%;  
	vertical-align: middle;
	padding: 100px 120px;
}

.menu-instruction-text{
	text-transform: uppercase;
	position: absolute;
	top:50px;
	left:calc(50% - 237.5px);
