/* =============================================================================
	 Header styles
	 ========================================================================== */

#header{
	font-family: 'divvyfont';
	letter-spacing: 3px;
	z-index: 10;
	color:#f4f4f4;
	text-transform: uppercase;
	position:absolute;

	top:50px;
	margin-left:50%;
	left:-200px;
	z-index: 10000;
	-webkit-transform: translate3d(0, 0, 0);
}

.header-menu-break{
	height: 85px;
	width: 5px;
	background-color:blue;
	display: inline-block;
	margin-right: 30px;

}
.header-display-text{
	font-family: 'divvyfont';
	letter-spacing: 3px;  
	color:#f4f4f4;
	text-transform: uppercase;
	font-size: 65px;
	position: fixed;
	left:0;
	top:5px;


}
.header-text-container{  
	 display: inline-block;
}

.header-text{
	font-size: 55px;
	font-family: "divvyfont";  
	margin:0px;
	margin-bottom: 5px;
}

.header-sub-text{
	display: block;  
	margin:0px;
}

.bt-menu {
	position: relative;
	display: inline-block;  
	-webkit-backface-visibility: hidden;
	width: 85px;
	height: 90px;
	cursor: pointer;
	margin:0px;  
	margin-right: 10px;
}
.bt-menu.mousedown{
	 transform:scale(.98,.98);
		-ms-transform:scale(.98,.98);
		-webkit-transform:scale(.98,.98);
}

.bt-menu.active:after{
	content: "Menu";
	position: absolute;
	top: 64px;
	left: 15px;
	font-size: 16px;
	opacity: .6;
	font-family: "divvyfont";
}

.bt-menu-trigger {
	position: absolute;
	width: 48px;
	height: 50px;
	top:15px;
	left: 15px;
	z-index: 1000;  
	cursor: pointer;
	padding: 20px;

}

.bt-menu-trigger span {
	position: absolute;
	top: 50%;
	left: 0;
	display: block;
	width: 100%;
	height: 2px;
	margin-top: -2px;
	background-color: #fff;
	font-size: 0px;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	-webkit-transition: background-color 0.3s;
	transition: background-color 0.3s;
}

.bt-menu-open .bt-menu-trigger span {
	background-color: transparent;
}

.bt-menu-trigger span:before,
.bt-menu-trigger span:after {
	position: absolute;
	left: 0;
	width: 100%;
	height: 100%;
	background: #fff;
	content: '';
	-webkit-transition: -webkit-transform 0.3s;
	transition: transform 0.3s;
}

.bt-menu-trigger span:before {
	-webkit-transform: translateY(-350%);
	transform: translateY(-350%);
}

.bt-menu-trigger span:after {
	-webkit-transform: translateY(350%);
	transform: translateY(350%);
}

.bt-menu-open .bt-menu-trigger span:before {
	-webkit-transform: translateY(0) rotate(45deg);
	transform: translateY(0) rotate(45deg);
}

.bt-menu-open .bt-menu-trigger span:after {
	-webkit-transform: translateY(0) rotate(-45deg);
	transform: translateY(0) rotate(-45deg);
}

.header-loader{
	position: absolute;
	top: 0px;
	left: 0px;
	width:400px;
	height: 50px;
	z-index: 100;
	
}
.header-loader-txt{
	display: block;
	width:100%;
	
	margin-left: 85px;
	font-size: 16px;
	margin-top: -3px;
 -webkit-animation: loading-text-opacity 2s ease-in-out infinite;
}

.loader{
	top:0%;
}


.loader, .loader:before, .loader:after {
	display: inline-block;
	width: 100%;
	height: 6px;

	position: absolute;
	
	-webkit-animation: loading 2s cubic-bezier(.1,.85,.9,.15) infinite, loading-opacity 1s ease-in-out infinite alternate;
	background: linear-gradient(to right, #f4f4f4 0px, #f4f4f4 8px, transparent 8px)  no-repeat 0px 0px / 8px 8px; 
	content: ' ';
}
.loader {
	-webkit-animation-delay: .1s;
}
.loader:after {
	-webkit-animation-delay: .2s;
}

@-webkit-keyframes loading-text-opacity {
	0% {
		opacity: 1;
	}
	35% {
		opacity: 0;
	}
	95%{
		opacity: 0
	}
	100% {
		opacity: 1;
	}
}

@-webkit-keyframes loading-opacity {
	0% {
		opacity: 0;
	}
	50% {
		opacity: 1;
	}
	100% {
		opacity: 1;
	}
}
@-webkit-keyframes loading {
	0% {
		background-position: -50% 0px;
	}
	100% {
		background-position: 150% 0px;
	}
}

/* 2 more loading blocks */
.loader.more:before, .loader.more:after {
	content: ' â– ';
	color: #f4f4f4;
	top: 0;  
	line-height: 6px;
	font-size: 16px;
	font-family: "Times New Roman";
	vertical-align: top;
	-webkit-animation: loading 2s cubic-bezier(.1,.85,.9,.15) infinite, loading-font 2s cubic-bezier(.1,.85,.9,.15) infinite !important;
}
.loader.more:before {
	-webkit-animation-delay: 0s,.2s !important;
}
.loader.more {
	overflow: hidden;
	opacity: 0;
	-webkit-animation: loading 2s cubic-bezier(.1,.85,.9,.15) infinite, loading-opacity 1s ease-in-out infinite alternate;
	-webkit-animation-delay: .3s,.2s !important;
}
.loader.more:after {
	-webkit-animation-delay: .2s,.4s !important;
}
@-webkit-keyframes loading-font {
	0% {
		text-indent: calc(-50% - 5px);
	}
	100% {
		text-indent: calc(150% - 10px);
	}
}

