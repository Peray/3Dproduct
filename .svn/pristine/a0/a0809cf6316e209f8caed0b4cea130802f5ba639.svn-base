	overflow: hidden;
	height: 40px;  

}
.menu-item{
	padding-top: 75px;
	width:33.33333%;
	height: 100%;  
	display: inline-block;
	vertical-align: middle;
	font-size: 21px;
	text-transform: uppercase;
	text-align: center;
	cursor: pointer;
	overflow: hidden;
	 -webkit-transform: translate3d(0, 0, 0);
}

.menu-item .menu-btn{
	
	width:385px;
