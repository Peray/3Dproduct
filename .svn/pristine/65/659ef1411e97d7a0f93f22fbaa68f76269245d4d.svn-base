	<div class="am-cf am-padding">
		<div class="am-g">
			<div class="am-u-sm-12">
				<div class="am-cf">
					<span class="am-fl">实时在线用户</span>
				</div>
				<table class="am-table am-table-bordered am-table-radius am-table-striped am-top" id="myTable2">
