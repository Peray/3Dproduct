						<tr>
							<th>编号</th>
							<th>用户名称</th>
							<th>上线时间</th>
							<th>持续时长（时分秒）</th>
							<th>操作</th>
						</tr>
					</thead>
					<tbody id="userAll">
					</tbody>
				</table>
				<ul class="am-pagination am-fr admin-content-pagination" id="pagination2">
				</ul>
