 }


/* 分页样式*/
 #pagination2{
 	padding-left: 0;
 	margin: 1.5rem 0;
 	list-style: none;
 	color: #999;
 	text-align: left;
 	float:right;
 }
 #pagination2>li{
 	display: inline-block;
 }
 #pagination2 li a{
 	padding: 4px 8px;
 }
 #pagination2>li>a{
 	position: relative;
 	display: inline-block;
 	padding: .5em 1em;
 	text-decoration: none;
 	line-height: 1.2;
 	background-color: #fff;
 	border: 1px solid #ddd;
 	border-radius: 0;
 	margin-bottom: 5px;
 	margin-right: 5px;
 }
 #pagination2>li.am-active>a{
 	z-index: 2;
 	color: #fff;
 	background-color: #0e90d2;
 	border-color: #0e90d2;
 	cursor: default;
 }
 #pagination2>li.am-disabled>a{
 	color: #999;
 	background-color: #fff;
 	border-color: #ddd;
 	cursor: not-allowed;
 	pointer-events: none;
 }
 /* 分页样式*/


/* 按钮*/
.btn {
	display: inline-block;
	padding: 8px 28px;
	margin-bottom: 0;
	font-size: 14px;
	font-weight: 400;
	line-height: 1.42857143;
	text-align: center;
	white-space: nowrap;
	vertical-align: middle;
	-ms-touch-action: manipulation;
	touch-action: manipulation;
	cursor: pointer;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	background-image: none;
	border: 1px solid transparent;
	border-radius: 1px;
}
.btn-primary {
	color: #fff;
	background-color: #337ab7;
	border-color: #2e6da4;
}
.btn-primary:hover{
	background-color: #285f8f;
}
.btn-primary[disabled]{
    pointer-events: none;
    border-color: transparent;
    cursor: not-allowed;
    opacity: .45;
    -webkit-box-shadow: none;
    box-shadow: none;
}







 /*弹出框*/
 .peray-open{
 	overflow:hidden;
 }
 .peray-open .peray {
    overflow-x: hidden;
    overflow-y: auto;
}
 .peray{
 	position: fixed;
 	top: 0;
 	right: 0;
 	bottom: 0;
 	left: 0;
 	z-index: 1050;
 	display: none;
 	overflow: hidden;
 	-webkit-overflow-scrolling: touch;
 	outline: 0;
	background-color: rgba(0,0,0,.4);
 }
 .fade{
 	opacity: 0;
 	-webkit-transition: opacity .15s linear;
 	-o-transition: opacity .15s linear;
 	transition: opacity .15s linear;
 }
 .fade.in{
 	opacity: 1;
 }
  .fade.in .peray-dialog{
 	-webkit-transform: translate(0,0);
 	-ms-transform: translate(0,0);
 	-o-transform: translate(0,0);
 	transform: translate(0,0);
 }
 .fade .peray-dialog{
 	-webkit-transition: -webkit-transform .5s ease-out;
 	-o-transition: -o-transform .5s ease-out;
 	transition: transform .5s ease-out;
 	-webkit-transform: translate(0,-105%);
 	-ms-transform: translate(0,-105%);
 	-o-transform: translate(0,-105%);
 	transform: translate(0,-105%);
 }

 .peray-dialog {
 	position: relative;
 	width: auto;
 	margin: 10px;
 }
 .peray-content{
 	position: relative;
 	background-color: #fff;
 	-webkit-background-clip: padding-box;
 	background-clip: padding-box;
 	border: 1px solid #999;
 	border: 1px solid rgba(0,0,0,.2);
 	border-radius: 6px;
 	outline: 0;
 	-webkit-box-shadow: 0 3px 9px rgba(0,0,0,.5);
 	box-shadow: 0 3px 9px rgba(0,0,0,.5);
 }
 .peray-header{
 	padding: 15px;
 	border-bottom: 1px solid #e5e5e5;
 }
 .peray-header .close {
 	margin-top: -2px;
 }
 .peray-body{
 	padding:20px;
 }
 .peray-body p.list{
 	line-height:50px;
 	font-size:16px;
 	border-bottom: 1px solid rgba(238, 238, 238, 0.4); 
 }
 .peray-body p.list>span{
 	display:inline-block;
 }
  .peray-body p.list>span:first-child{
  	width:140px;
  	text-align: right;
  }
  .peray-body p.list>span:last-child{
  	padding-left:30px;
  }
  .peray-footer{
  	text-align: center;
  	padding:10px 0 20px;
  }
 button.close {
 	-webkit-appearance: none;
 	padding: 0;
 	cursor: pointer;
 	background: 0 0;
 	border: 0;
 }
 .close {
 	float: right;
 	font-size: 21px;
 	font-weight: 700;
 	line-height: 1;
 	color: #000;
 	text-shadow: 0 1px 0 #fff;
 	filter: alpha(opacity=20);
 	opacity: .2;
 }
  .close:hover{
  	opacity: 1;
  }
 .peray-title {
 	margin: 0;
 	line-height: 1.42857143;
 	font-size: 18px;
 	font-weight:500;
 }



 @media (min-width: 768px){
 	.peray-dialog {
 		width: 600px;
 		margin: 30px auto;
 	}
 	.peray-content{
 		-webkit-box-shadow: 0 5px 15px rgba(0,0,0,.5);
 		box-shadow: 0 5px 15px rgba(0,0,0,.5);
 	}
 }
