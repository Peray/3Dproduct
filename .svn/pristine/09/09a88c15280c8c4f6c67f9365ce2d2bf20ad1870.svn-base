			ret = eval(ret);
				$.ajax({
					type:'post',
					url: 'categoryAdmin!execute.action',
					dataType: 'json',
			//		data: {categoryId:1},
					success:function(rets){
						rets = eval(rets);
						var div = "";
						for (var i = 0; i < rets.list.length; i++) {
							if(rets.list[i].categoryName != "时相" && rets.list[i].categoryName != "服务类型"){
