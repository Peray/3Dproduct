		    		var fileTArr=fileArr[fileArr.length-1].toLowerCase().split(".");
		    		var filetype=fileTArr[fileTArr.length-1];
		   		 //切割出后缀文件名
		   		 if(filetype != "geojson"){
		        		$(this).val("");
		        		$tooltip.text("只允许上传geojson格式的文件").show().css({
						left: offset.left + 10,
						top: offset.top + $(this).outerHeight()-5
					});
		    		}else{
		        		$tooltip.hide();
			    		 //创建和初始化地图
		    		}
			})
