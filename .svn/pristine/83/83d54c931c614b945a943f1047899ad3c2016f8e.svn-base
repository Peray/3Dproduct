*{margin:0; padding:0; list-style:none;}
.out{width:100%; height:200px;position:relative;}
.tu li{position:absolute; top:0; left:0; display:none;width:100%;}
.shu{position:absolute; bottom:10px; left:0; text-align:right; width:100%; font-size:0;}
.shu li{width:20px; height:20px; background:#333; color:#fff; text-align:center; line-height:20px; font-size:14px; border-radius:50%; display:inline-block; margin:0 5px; cursor:pointer;}
.shu li.active{background:#a00;}
.out .btn{width:30px; height:60px; background:rgba(0,0,0,0.3); color:#fff; font-size:20px; text-align:center; line-height:60px; position:absolute; top:50%; margin-top:-30px; cursor:pointer;}
.out .left{left:0;}
.out .right{right:0;}






